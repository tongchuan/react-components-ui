  "main": "./lib/index",
  "module": "./es/index",
  "files": [
    "assets/*.css",
    "assets/*.png",
    "assets/*.gif",
    "es",
    "lib"
  ],
  "config": {
    "port": 8000,
    "entry": {
      "react-components-ui": [
        "./assets/index.less",
        "./src/index.jsx"
      ]
    }
  },
  "scripts": {
    "version4444": "npm version [<newversion> | major | minor | patch | premajor | preminor | prepatch | prerelease [--preid=<prerelease-id>] | from-git]",
