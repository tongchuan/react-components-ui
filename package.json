    "url": "https://github.com/tongchuan"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tongchuan/react-components-ui/issues"
  },
  "homepage": "https://github.com/tongchuan/react-components-ui#readme",
  "dependencies": {
    "eslint": "^5.15.1",
    "mini-store": "^2.0.0",
    "rc-tools": "^9.3.11",
